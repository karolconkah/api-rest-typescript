    "ts-jest": "^29.4.6",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/jest": "^30.0.0",
    "@types/express": "4.17.13",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "5.30.6",
    "@typescript-eslint/parser": "5.30.6",
    "eslint": "8.19.0",
